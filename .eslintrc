{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "quotes": [ "error", "single", { "allowTemplateLiterals": true } ],
        "no-console": 0,
        "semi": [ "error", "always" ]
    }
}